      ]
    },
    {
      "cell_type": "code",
      "source": [
        "#Gradient Boosting\n",
        "from sklearn.ensemble import GradientBoostingRegressor\n",
        "\n",
        "gb = GradientBoostingRegressor(random_state=42)\n",
        "gb.fit(X_train, y_train)\n",
        "\n",
        "y_gb = gb.predict(X_test)\n",
        "\n",
        "print(\"GB R2:\", r2_score(y_test, y_gb))\n"
      ],
