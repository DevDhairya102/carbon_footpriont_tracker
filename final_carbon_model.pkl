              "       'Frequency of Traveling by Air', 'Waste Bag Size', 'Energy efficiency',\n",
              "       'Recycling', 'Cooking_With'],\n",
              "      dtype='object')"
            ]
          },
          "metadata": {},
          "execution_count": 15
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "#one hot encoding-pd.get_dummies\n",
        "y = df[\"CarbonEmission\"]\n",
        "X = df.drop(\"CarbonEmission\",axis=1)\n",
        "\n",
        "X_encoded = pd.get_dummies(X,drop_first=True)\n",
        "print(X.shape)\n",
        "print(X_encoded.shape)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "b8Xp_igh5sZo",
        "outputId": "f5a707ae-6627-426c-f743-238bc22161a3"
      },
      "execution_count": 33,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "(10000, 19)\n",
            "(10000, 66)\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "#MODEL split\n",
        "from sklearn.model_selection import train_test_split\n",
        "X_train,X_test,y_train,y_test = train_test_split(X_encoded,y,test_size=0.2,random_state=42)\n"
      ],
      "metadata": {
        "id": "UDU_kJoF750O"
      },
      "execution_count": 34,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#Random Forest\n",
        "from sklearn.metrics import mean_absolute_error,mean_squared_error,r2_score\n",
        "from sklearn.ensemble import RandomForestRegressor\n",
        "rf_model = RandomForestRegressor(n_estimators=200,random_state=42,n_jobs=-1)\n",
        "rf_model.fit(X_train,y_train)\n",
        "\n",
        "ypred = rf_model.predict(X_test)\n",
        "print(\"MAE:\",mean_absolute_error(y_test,y_test))\n",
        "print(\"RMSE:\",mean_squared_error(y_test,ypred))\n",
        "print(\"R2:\",r2_score(y_test,ypred))"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "eJYM49sK63Av",
