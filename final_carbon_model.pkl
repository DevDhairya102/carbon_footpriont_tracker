      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "df = pd.read_csv(\"Carbon Emission.csv\")\n",
        "print(df.isnull().sum())"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# filling vehicle nans\n",
        "df[\"Vehicle Type\"] = df[\"Vehicle Type\"].fillna(\"No Vehicle\")\n",
        "print(df[\"Vehicle Type\"].value_counts())"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "collapsed": true,
        "id": "iHA0-Frq2HW-",
